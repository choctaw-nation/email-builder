{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"color": {
			"custom": true,
			"customDuotone": false,
			"customGradient": false,
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [],
			"gradients": [],
			"palette": [
				{
					"color": "white",
					"name": "White",
					"slug": "white"
				},
				{
					"color": "black",
					"name": "Black",
					"slug": "black"
				},
				{
					"color": "#c8c8c8",
					"name": "Gray",
					"slug": "gray"
				}
			]
		},
		"dimensions": {
			"aspectRatio": false
		},
		"layout": {
			"allowCustomContentAndWideSize": false,
			"allowEditing": false,
			"wideSize": "600px",
			"contentSize": "600px"
		},
		"shadow": {
			"defaultPresets": false
		},
		"spacing": {
			"blockGap": false,
			"customSpacingSize": false,
			"defaultSpacingSizes": false,
			"margin": true,
			"padding": true,
			"units": [ "px" ],
			"spacingScale": {
				"operator": "*",
				"increment": 1.5,
				"unit": "px",
				"mediumStep": 10,
				"steps": 5
			}
		},
		"typography": {
			"customFontSize": true,
			"defaultFontSizes": true,
			"dropCap": false,
			"fontWeight": true,
			"lineHeight": true,
			"textDecoration": false
		},
		"useRootPaddingAwareAlignments": false
	},
	"styles": {
		"spacing": {
			"blockGap": "var:preset|spacing|base"
		}
	}
}

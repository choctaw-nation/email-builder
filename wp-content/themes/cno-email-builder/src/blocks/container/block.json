{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "cno-email-blocks/container",
	"version": "2.0.0",
	"title": "Container",
	"category": "cno-email-blocks",
	"icon": "block-default",
	"description": "Container block that wraps other email blocks.",
	"example": {},
	"supports": {
		"html": false,
		"multiple": true,
		"className": false,
		"inserter": true,
		"spacing": {
			"margin": true,
			"padding": true
		}
	},
	"attributes": {
		"backgroundColor": {
			"type": "string",
			"attribute": "style.backgroundColor",
			"default": "white"
		},
		"maxWidth": {
			"type": "number",
			"default": 600
		},
		"margin": {
			"type": "object"
		},
		"padding": {
			"type": "object"
		}
	},
	"render": "file:./render.php",
	"ancestor": [ "cno-email-blocks/email-wrapper" ],
	"editorScript": "file:./index.tsx"
}

{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "cno-email-blocks/container",
	"version": "0.1.0",
	"title": "CNO Email Container",
	"category": "cno-email-blocks",
	"icon": "block-default",
	"description": "Container block that wraps other email blocks.",
	"example": {},
	"supports": {
		"html": false,
		"multiple": true,
		"className": false,
		"inserter": true,
		"spacing": {
			"padding": true,
			"margin": false
		},
		"color": {
			"background": true
		}
	},
	"attributes": {
		"backgroundColor": {
			"type": "string",
			"attribute": "style.backgroundColor",
			"default": "white"
		},
		"maxWidth": {
			"type": "number",
			"default": 600
		}
	},
	"ancestor": [ "cno-email-blocks/body" ],
	"editorScript": "file:./index.tsx"
}

{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "cno-email-blocks/font",
	"version": "0.2.0",
	"title": "CNO Email Font",
	"category": "cno-email-blocks",
	"icon": "block-default",
	"description": "A way to add fonts to your email.",
	"example": {},
	"supports": {
		"html": false,
		"className": false,
		"inserter": false,
		"multiple": false
	},
	"attributes": {
		"useDefaultFonts": {
			"type": "boolean",
			"default": true
		},
		"fontUrl": {
			"type": "string",
			"default": ""
		},
		"headingsFont": {
			"type": "object",
			"default": {
				"fontWeight": "",
				"fallbackStack": "",
				"fontFace": ""
			}
		},
		"bodyFont": {
			"type": "object",
			"default": {
				"fontWeight": "",
				"fallbackStack": "",
				"fontFace": ""
			}
		},
		"accentFont": {
			"type": "object"
		}
	},
	"ancestor": [ "cno-email-blocks/head" ],
	"editorScript": "file:./index.tsx"
}

{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "cno-email-blocks/divider",
	"version": "0.1.0",
	"title": "CNO Email Divider",
	"category": "cno-email-blocks",
	"icon": "block-default",
	"description": "Divider block",
	"example": {},
	"supports": {
		"html": false,
		"className": false,
		"spacing": {
			"margin": true
		}
	},
	"attributes": {
		"border": {
			"type": "object",
			"default": {
				"color": "#000000",
				"style": "solid",
				"width": 1
			},
			"attribute": "border",
			"selector": "hr"
		},
		"width": {
			"type": "string",
			"default": "100%",
			"attribute": "width",
			"selector": "hr"
		}
	},
	"ancestor": [ "cno-email-blocks/body" ],
	"editorScript": "file:./index.tsx"
}

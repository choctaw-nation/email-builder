{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "cno-email-blocks/body",
	"version": "0.1.0",
	"title": "CNO Email Body",
	"category": "cno-email-blocks",
	"icon": "block-default",
	"description": "Email Body adds some controls for when the email is viewed in a browser.",
	"example": {},
	"supports": {
		"html": false,
		"inserter": false,
		"className": false,
		"color": {
			"background": true,
			"text": false
		}
	},
	"attributes": {
		"backgroundColor": {
			"type": "string",
			"attribute": "style.backgroundColor",
			"default": "gray"
		},
		"lock": {
			"type": "object",
			"default": {
				"move": true,
				"remove": true
			}
		}
	},
	"usesContext": [ "cno-email-blocks/previewText" ],
	"parent": [ "cno-email-blocks/email-wrapper" ],
	"editorScript": "file:./index.tsx",
	"render": "file:./render.php"
}

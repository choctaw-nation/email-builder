{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "cno-email-blocks/row",
	"version": "1.0.0",
	"title": "Row",
	"category": "cno-email-blocks",
	"icon": "block-default",
	"description": "Row block for email content",
	"example": {},
	"supports": {
		"html": false,
		"inserter": true,
		"className": false
	},
	"attributes": {
		"columnGap": {
			"type": "number",
			"default": 10
		},
		"rowGap": {
			"type": "number",
			"default": 10
		},
		"canWrap": {
			"type": "boolean",
			"default": true
		},
		"maxWidth": {
			"type": "number",
			"default": 600
		},
		"margin": {
			"type": "object"
		},
		"padding": {
			"type": "object"
		}
	},
	"providesContext": {
		"cno-email-blocks/columnGap": "columnGap",
		"cno-email-blocks/rowGap": "rowGap",
		"cno-email-blocks/canWrap": "canWrap"
	},
	"ancestor": [ "cno-email-blocks/email-wrapper" ],
	"editorScript": "file:./index.tsx"
}

{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "cno-email-blocks/email-wrapper",
	"version": "1.0.1",
	"title": "Email",
	"category": "cno-email-blocks",
	"icon": "block-default",
	"description": "Wrapper block for email content",
	"example": {},
	"keywords": [ "email" ],
	"supports": {
		"html": false,
		"multiple": false,
		"className": false
	},
	"attributes": {
		"previewText": {
			"type": "string",
			"default": ""
		},
		"backgroundColor": {
			"type": "string",
			"default": "#C8C8C8"
		},
		"useDefaultFonts": {
			"type": "boolean",
			"default": true
		},
		"fontUrl": {
			"type": "string",
			"default": "https://use.typekit.net/exd7pgy.css"
		},
		"headingsFont": {
			"type": "object",
			"default": {
				"name": "pill-gothic-300mg",
				"title": "Pill Gothic 300mg",
				"fontWeights": [ 400, 700 ],
				"fontStyles": [ "normal", " italic" ],
				"fallbackStack": {
					"label": "sans-serif",
					"value": "Arial, Helvetica, sans-serif"
				}
			}
		},
		"bodyFont": {
			"type": "object",
			"default": {
				"name": "gill-sans-nova",
				"title": "Gill Sans Nova",
				"fontWeights": [ 400, 700 ],
				"fontStyles": [ "normal", " italic" ],
				"fallbackStack": {
					"label": "sans-serif",
					"value": "Arial, Helvetica, sans-serif"
				}
			}
		},
		"accentFont": {
			"type": "object"
		},
		"responsiveBlocks": {
			"type": "array",
			"default": []
		},
		"hasCustomColors": {
			"type": "boolean",
			"default": false
		},
		"customColorPalette": {
			"type": "object",
			"default": {
				"primary": "",
				"secondary": "",
				"tertiary": ""
			}
		}
	},
	"providesContext": {
		"cno-email-blocks/previewText": "previewText",
		"cno-email-blocks/title": "title",
		"cno-email-blocks/fontUrl": "fontUrl",
		"cno-email-blocks/headingsFont": "headingsFont",
		"cno-email-blocks/bodyFont": "bodyFont",
		"cno-email-blocks/accentFont": "accentFont"
	},
	"editorScript": "file:./index.tsx",
	"editorStyle": "file:./index.css",
	"render": "file:./render.php"
}

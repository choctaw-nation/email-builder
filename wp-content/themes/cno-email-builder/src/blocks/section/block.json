{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "cno-email-blocks/section",
	"version": "0.1.0",
	"title": "CNO Email Section",
	"category": "cno-email-blocks",
	"icon": "block-default",
	"description": "Section block for email content",
	"example": {},
	"supports": {
		"html": false,
		"inserter": true,
		"className": false,
		"spacing": {
			"margin": true,
			"padding": true
		},
		"align": true,
		"alignWide": false
	},
	"attributes": {
		"style": {
			"type": "object",
			"default": {
				"spacing": {
					"padding": {
						"top": "0",
						"right": "var:preset|spacing|50",
						"bottom": "0",
						"left": "var:preset|spacing|50"
					},
					"margin": {
						"top": "var:preset|spacing|70",
						"bottom": "var:preset|spacing|70"
					}
				}
			}
		}
	},
	"ancestor": [ "cno-email-blocks/body" ],
	"editorScript": "file:./index.tsx"
}

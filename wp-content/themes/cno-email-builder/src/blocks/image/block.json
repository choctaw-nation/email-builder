{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"version": "1.0.0",
	"name": "cno-email-blocks/image",
	"title": "Image",
	"icon": "image",
	"category": "cno-email-blocks",
	"description": "Insert an image.",
	"keywords": [ "img", "photo", "picture" ],
	"attributes": {
		"blob": {
			"type": "string",
			"role": "local"
		},
		"url": {
			"type": "string"
		},
		"alt": {
			"type": "string"
		},
		"title": {
			"type": "string"
		},
		"id": {
			"type": "number"
		},
		"width": {
			"type": "string"
		},
		"height": {
			"type": "string"
		},
		"scale": {
			"type": "string"
		},
		"href": {
			"type": "string",
			"source": "attribute",
			"selector": "a",
			"attribute": "href",
			"role": "content"
		},
		"linkDestination": {
			"type": "string"
		},
		"linkTarget": {
			"type": "string",
			"source": "attribute",
			"selector": "a",
			"attribute": "target"
		},
		"rel": {
			"type": "string",
			"source": "attribute",
			"selector": "a",
			"attribute": "rel"
		}
	},
	"ancestor": [ "cno-email-blocks/email-wrapper" ],
	"supports": {
		"anchor": false,
		"html": false,
		"className": false,
		"spacing": {
			"margin": true
		}
	},
	"editorScript": [ "file:./index.tsx" ],
	"render": "file:./render.php"
}
